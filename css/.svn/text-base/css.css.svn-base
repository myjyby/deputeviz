/* ----- SLIDER ----- */
#scale-slider{
  height: 30px;
  /*width: 100%;*/
  /*background-color: #EAEAEA;*/
}

.slider-bar{
  margin-top: 11px;
  margin-left: 5px;
  width: calc(100% - 15px);

  /*height: 1px;
  background-color: #CCC;*/
}

.ref-thumb{
  position: absolute;
  width: calc(100% - 37px);
}

.ref-sm-thumb{
    -webkit-appearance: none;
    border: none;
    height: 9px;
    width: 9px;
    border-radius: 50%;
    border: 2px dotted #999;
    margin-left: -2px;
    margin-top: 8px;
    float: left;
}
.ref-lg-thumb{
    -webkit-appearance: none;
    border: none;
    height: 24px;
    width: 24px;
    border-radius: 50%;
    border: 2px dotted #999;
    margin-left: 2px;
    float: right;
}

.slider-thumb{
    position: absolute;
    border: none;
    height: 12.5px;
    width: 12.5px;
    border-radius: 50%;
    background: #333;
    /*border: 2px dotted #333;*/
    margin-top: -6.75px;
    z-index: 100;
    cursor: col-resize;
}

.no-wrap{
  white-space: nowrap;
}

.no-left-padding{
  padding-left: 0;
}

.no-right-padding{
  padding-right: 0;
}

