.brush{
  /*opacity: 0.1;*/
  fill: none;
  stroke: #333;
  stroke-width: 0.5;
}


.simu-brush{
  fill: #FFF;
  fill-opacity: 0.75;
  stroke: #333;
  stroke-width: 0.5;
}

.simu-cursor line{
  stroke: #000;
  stroke-width: 0.75;
}

.noselect {
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    cursor: default;
}

.animate-point-scale{
  animation: grow 3s infinite ease-in-out;
  -webkit-animation: grow 3s infinite ease-in-out;
  -moz-animation: grow 3s infinite ease-in-out;
  -o-animation: grow 3s infinite ease-in-out;
}

@keyframes grow{
  0%{
    width: 10px;
    height: 10px;
    left: 50%;
    top: 50%;
    margin-left: -5px;
    margin-top: -5px;
  }15%{
    width: 30px;
    height: 30px;
    left: 50%;
    top: 50%;
    margin-left: -15px;
    margin-top: -15px;
  }50%{
    width: 10px;
    height: 10px;
    left: 50%;
    top: 50%;
    margin-left: -5px;
    margin-top: -5px;
  }
}

@-webkit-keyframes grow{
  0%{
    width: 10px;
    height: 10px;
    left: 50%;
    top: 50%;
    margin-left: -5px;
    margin-top: -5px;
  }15%{
    width: 30px;
    height: 30px;
    left: 50%;
    top: 50%;
    margin-left: -15px;
    margin-top: -15px;
  }50%{
    width: 10px;
    height: 10px;
    left: 50%;
    top: 50%;
    margin-left: -5px;
    margin-top: -5px;
  }
}

@-moz-keyframes grow{
  0%{
    width: 10px;
    height: 10px;
    left: 50%;
    top: 50%;
    margin-left: -5px;
    margin-top: -5px;
  }15%{
    width: 30px;
    height: 30px;
    left: 50%;
    top: 50%;
    margin-left: -15px;
    margin-top: -15px;
  }50%{
    width: 10px;
    height: 10px;
    left: 50%;
    top: 50%;
    margin-left: -5px;
    margin-top: -5px;
  }
}

@-o-keyframes grow{
  0%{
    width: 10px;
    height: 10px;
    left: 50%;
    top: 50%;
    margin-left: -5px;
    margin-top: -5px;
  }15%{
    width: 30px;
    height: 30px;
    left: 50%;
    top: 50%;
    margin-left: -15px;
    margin-top: -15px;
  }50%{
    width: 10px;
    height: 10px;
    left: 50%;
    top: 50%;
    margin-left: -5px;
    margin-top: -5px;
  }
}

.animate-point-color{
  animation: glow 3s 1.5s infinite ease-in-out;
  -webkit-animation: glow 3s 1.5s infinite ease-in-out;
  -moz-animation: glow 3s 1.5s infinite ease-in-out;
  -o-animation: glow 3s 1.5s infinite ease-in-out;
}

@keyframes glow{
  0%{
    background-color: #333;
  }15%{
    background-color: royalblue;
  }50%{
    background-color: #333;
  }
}

@-webkit-keyframes glow{
  0%{
    background-color: #333;
  }15%{
    background-color: royalblue;
  }50%{
    background-color: #333;
  }
}

@-moz-keyframes glow{
  0%{
    background-color: #333;
  }15%{
    background-color: royalblue;
  }50%{
    background-color: #333;
  }
}

@-o-keyframes glow{
  0%{
    background-color: #333;
  }15%{
    background-color: royalblue;
  }50%{
    background-color: #333;
  }
}

.strong-border{
  stroke: #333;
  fill: none;
}
.strong-white-border{
  stroke: #FFF;
  stroke-width: 2;
  fill: none;
}

.x-indicator{
  background-color: transparent;
}
.y-indicator{
  background-color: transparent;
}

#navigation .btn-group > .btn:last-child:not(:first-child), #navigation .btn-group > .dropdown-toggle:not(:first-child){
  border-top-right-radius: 0;
}

.dropup.x-axis-scale{
  margin-top: 17px;
}

.dropup .dropdown-menu-x-axis{
  bottom: -19px !important;
}

.dropdown-menu-y-axis{
  top: 19px;
}

.no-margin-bottom{
  margin-bottom: 0;
}

.separator-with-padding{
