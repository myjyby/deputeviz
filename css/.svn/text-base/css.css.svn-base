}

.svg-title{
  font-family: 'Abril Fatface', Georgia, "Times New Roman", Times, serif;
  font-size: 4em;
  /*font-weight: 700;*/
  fill: #111;
}

.svg-sub-title{
  font-family: 'Abril Fatface', Georgia, "Times New Roman", Times, serif;
  font-size: 2em;
  fill: #111;
  font-weight: 700;
}

.droplist-action-title{
  font-size: 1.2em;
  border-bottom: 2px solid #333;
  margin-bottom: 10px;
}

.scale.selected a, .color.selected a, .more-data.selected, .y-range.selected a, .x-range.selected a, .scrutin-opt.selected a{
  background-color: tomato;
  color: #333 !important;
}

.scale.selected:hover a, .color.selected:hover a, .more-data.selected:hover, .y-range.selected:hover a, .x-range.selected:hover a, .scrutin-opt.selected:hover a{
  background-color: tomato !important;
  color: #333 !important;
}

.scrutin-opt.selected a table .table-first-cell{  
  border-right-color: #333;
}

.hide{
  display: none;
}

.color-menu{
  min-width: 200px;
}

