.container.viz{
  /*position: relative;*/
  padding-bottom: 10px;
  border-bottom: 1px solid #CCC;
}

.container{
  background-color: #FFF;
}

path{
fill: none;
}
path.domain{
fill: none;
stroke: #000;
stroke-width: 0.5px;
}
line{
stroke: #000;
stroke-width: 0.5px;
}
text{
font-family: Helvetica, sans-serif;
}

.node:not(.active-dot) {
  fill: #ccc !important;
}

.map-borders{
  stroke: #CCC;
  /*fill: #FFF;*/
}

.node.missing-dimension, .node.missing-scale{
  stroke: #333 !important;
  fill: #FFF !important;
  fill-opacity: 0 !important;
}


.link {
fill: none;
stroke: #ccc;
stroke-width: 1.5px;
}

.outre-mer-circle{
  fill: none;
  stroke: #CCC;
}

svg{
overflow: visible;
/*shape-rendering: crispEdges;*/
}

/*circle{
opacity: 0.75;
}*/

body {
font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
margin: auto;
position: relative;
/*background-color: #333;*/
}

body.f-obj-body{
  background-color: transparent;
  position: none;
  padding: 0;
}

form {
position: absolute;
right: 10px;
top: 10px;
}

#header{
  background-color: #333;
  color: #FFF;
  letter-spacing: 0.1em;
}

#header p.lead{
  margin-bottom: 3px;
  margin-top: 2px;
  font-weight: lighter;
}

.node {
border: solid 1px white;
font: 10px sans-serif;
line-height: 12px;
overflow: hidden;
position: absolute;
text-indent: 2px;
stroke: #ccc;
cursor: pointer;
}

.label{
  /*text-shadow: 0px 0px 1px #fff;*/
  text-shadow: -1px 0 #FFF, 0 1px #FFF, 1px 0 #FFF, 0 -1px #FFF;
  color: #333;
  padding: 0 0 0 1px;
  margin-right: 0;
  text-align: left;
  font-size: 1em;
  white-space: normal;
  color: #000;
}

.dep-label{
  /*text-shadow: 0px 0px 1px #fff;*/
  text-shadow: -1px 0 #FFF, 0 1px #FFF, 1px 0 #FFF, 0 -1px #FFF;
  color: #333;
  padding: 0 0 0 1px;
  margin-right: 0;
  text-align: center;
  font-size: 1em;
  white-space: normal;
  color: #000;
}

.scale-btn{
  border-radius: 35px;
  border: 2px dotted #333;
  width: 34px;
  height: 34px;
  position: relative;
  padding: 0;
}

.scale-btn:hover{
  background-color: transparent;
}

.scale-btn:hover .dot-btn{
  width: 30px;
  height: 30px;
  left: 50%;
  top: 50%;
  margin-left: -15px;
  margin-top: -15px;
}

.dot-btn{
  background-color: #333;
  border-radius: 35px;
  border: 2px dotted #333;
  width: 10px;
  height: 10px;
  left: 50%;
  top: 50%;
  margin-left: -5px;
  margin-top: -5px;
  position: absolute;
}

.legend{
  height: 100px;
}

.dot-legend{
  background-color: #333;
  border-radius: 35px;
  border: 2px solid #333;
  width: 12px;
  height: 12px;
  margin-top: 4px;
  margin-right: 5px;
  display: block;
  float: left;
}

.dot-legend.missing-val{
  border: 1px solid #333;
  background-color: transparent;
  width: 11px;
  height: 11px;
  margin-left: 1px;
}

.legend-entry{
  display: block;
}

.legend-entry:last-child(){
  padding-right: 20px;
}

.color-btn{
  border-radius: 35px;
  border: none;
  width: 34px;
  height: 34px;
  position: relative;
  padding: 0;
}

.color-btn .dot-btn{
  background-color: #333;
  border: 1px solid #CCC;
  width: 20px;
  height: 20px;
  left: 50%;
  top: 50%;
  margin-left: -10px;
  margin-top: -10px;
}

.color-btn:hover{
  background-color: transparent;
}

.color-btn:hover .dot-btn{
  background-color: royalblue;
}

.animated{
  -webkit-transition: all 0.5s; /* For Safari 3.1 to 6.0 */
  -moz-transition: all 0.5s;
  -o-transition: all 0.5s; 
  transition: all 0.5s;
}

.animated-slow{
  -webkit-transition: all 1s; /* For Safari 3.1 to 6.0 */
  -moz-transition: all 1s;
  -o-transition: all 1s; 
  transition: all 1s;
}

.drop-list-padding{
  padding: 3px 20px;
}

.border-top{
  border-top: 1px solid #CCC;
}

