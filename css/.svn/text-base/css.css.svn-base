}

.active-dot.missing-scale.ps{
  stroke: crimson !important;
  stroke-width: 2 !important;
}

.active-dot.missing-scale.ump{
  stroke: midnightblue !important;
  stroke-width: 2 !important;
}

.active-dot.missing-scale.a-ump{
  stroke: darkslateblue !important;
  stroke-width: 2 !important;
}

.active-dot.missing-scale.a-ps{
  stroke: firebrick !important;
